generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id          Int      @id @default(autoincrement())
  fullUrl     String
  shortCode   String   @unique
  clicks      Int      @default(0)
  lastClicked DateTime?
  createdAt   DateTime @default(now())
}
